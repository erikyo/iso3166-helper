{
	"name": "iso3166-helper",
	"version": "1.0.0",
	"author": "Erik",
	"license": "ISC",
	"description": "Provides utilities to work with regions, states, and sub-regions. It allows fetching regions and sub-regions by their codes and offers functionality to retrieve all sub-regions belonging to a specific region or state.",
	"type": "module",
	"sideEffects": false,
	"main": "lib/index.js",
	"module": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib/**/*",
		"tests/**/*",
		"*.json",
		"*.md"
	],
	"keywords": [
		"mapbox",
		"geocoder",
		"iso3661",
		"country",
		"regions",
		"subregions"
	],
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"start": "tsc --watch",
		"build": "tsc",
		"lint": "npx @biomejs/biome check --fix --unsafe src test",
		"test": "node --experimental-vm-modules --experimental-specifier-resolution=node --test",
		"prepublishOnly": "npm run build",
		"pretest": "npm run build"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"typescript": "^5.5.2"
	}
}
